            "whatTests": ["pad"],
            "template": [
                "http://example.com/data_$(S;pad=none).dat",                              
            ],
            "timeRange": [
                "1999-01-01T00:00:00/1999-01-01T00:00:03"
            ],
            "comment": "",
            "output": [ 
